		"@gltf-transform/core": "^3.10.0",
		"@gltf-transform/extensions": "^3.10.0",
		"@gltf-transform/functions": "^3.10.0",
		"draco3dgltf": "^1.5.7",
		"es-module-shims": "^1.8.2",
		"lit": "^3.1.2",
		"simplex-noise": "^4.0.1",
		"sparrow-rtc": "^0.0.0-dev.6"
	},
	"peerDependencies": {
		"@babylonjs/core": "^6.43.0",
		"@babylonjs/loaders": "^6.43.0"
	},
	"devDependencies": {
		"@benev/turtle": "^0.5.2",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-wasm": "^6.2.2",
		"chokidar": "^3.6.0",
		"chokidar-cli": "^3.0.0",
		"cynic": "^0.2.1",
		"http-server": "^14.1.1",
		"importly": "^0.2.3",
		"mkdirp": "^3.0.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^5.0.5",
		"rollup": "^4.12.0",
		"terser": "^5.27.1",
		"typescript": "^5.3.3",
		"wrap-ansi": "^9.0.0"
	},
	"keywords": [
		"web-games",
		"toolkit",
		"babylon",
		"babylonjs"
	],
